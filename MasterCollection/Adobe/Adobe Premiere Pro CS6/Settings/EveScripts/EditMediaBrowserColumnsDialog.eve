layout my_dialog
{
	view dva_dialog(name: '$$$/Premiere/DLG/EditMediaBrowserColumnsDialog/kResDLGEditColumnsDialogTitle=Edit Columns', identifier:'EditMediaBrowserColumnsDialog', canResize:'true')
	{
		row()
		{
			column()
			{
				dva_popup(identifier:'providersPopup', minWidth:200, minHeigth:20, label: '$$$/Premiere/DLG/EditMediaBrowserColumnsDialog/kResProviderPopupLabel=Directory Viewers:');
				dva_listbox(identifier:'columnsList', direction:'vertical', useCheckboxes:'true', minHeight:391, horizontal:align_fill);
			}
			column(horizontal:align_top)
			{
				dva_ok_cancel_column();	
				dva_button(name: '$$$/Premiere/DLG/kResDLGEditMediaBrowserColumnsDialog/Moveup=Move Up', identifier:'moveup', topSpace:10);
				dva_button(name: '$$$/Premiere/DLG/kResDLGEditMediaBrowserColumnsDialog/Movedown=Move Down', identifier:'movedown');
			}
		}
	}
} 

